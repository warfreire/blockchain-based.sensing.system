{
  "name":"nmiblocknet_default",
  "description":"Connection profile to access the nmiblocknet from the PTB org",
  "version":"0.1",
  "client":{
    "organization":"PTB",
    "credentialStore":{
      "path":"/tmp/hfc-kvs",
      "cryptoStore":{
        "path":"/tmp/hfc-cvs"
      },
      "wallet":"wallet-name"
    }
  },
  "channels": {
    "ptb-channel": {
        "orderers": [
            "orderer.nmi"
        ],
        "peers": {
            "peer0.ptb.de": {
                "endorsingPeer": true,
                "chaincodeQuery": true,
                "ledgerQuery": true,
                "eventSource": true
            }
        }
    }
  },
  "organizations":{
    "orderer.nmi":{
      "mspid": "OrdererMSP",
      "orderers": [
        "orderer.nmi"
      ],
      "users": {
        "Admin": {
          "cert": "./crypto-config/ordererOrganizations/orderer.nmi/users/Admin@orderer.nmi/msp/signcerts/Admin@orderer.nmi-cert.pem",
          "private_key": "./crypto-config/ordererOrganizations/orderer.nmi/users/Admin@orderer.nmi/msp/keystore/d7ab1e1d387bbf4be6eca4cb1c15afd8581f2f469da57a7dff5090711d2d1bb9_sk"}
      }
    },
    "ptb.de":{
      "mspid": "PTBMSP",
      "peers":[
        "peer0.ptb.de",
        "peer1.ptb.de"
      ],
      "users": {
        "Admin": {
          "cert": "./crypto-config/peerOrganizations/ptb.de/users/Admin@ptb.de/msp/signcerts/Admin@ptb.de-cert.pem",
          "private_key": "./crypto-config/peerOrganizations/ptb.de/users/Admin@ptb.de/msp/keystore/90b8738d740bd3dc1ed3ebba625c5fd448f0f54b6ecebfbb9307cdb76a23a8e0_sk"
        }
      }
    }
  },
  "orderers":{
    "orderer.nmi":{
      "url":"200.20.211.164:7050",
      "grpcOptions": {
        "grpc.ssl_target_name_override": "orderer.nmi",
        "grpc-max-send-message-length": 15
      },
      "tlsCACerts": {
        "path": "./crypto-config/ordererOrganizations/orderer.nmi/tlsca/tlsca.orderer.nmi-cert.pem"
      },
      "clientKey": {
        "path": "./crypto-config/ordererOrganizations/orderer.nmi/orderers/orderer.nmi.orderer.nmi/tls/server.key"
      },
      "clientCert": {
        "path": "./crypto-config/ordererOrganizations/orderer.nmi/orderers/orderer.nmi.orderer.nmi/tls/server.crt"
      }
    }
  },
  "peers":{
    "peer0.ptb.de":{
      "url":"194.94.94.163:7051",
      "eventUrl":"194.94.94.163:7053",
